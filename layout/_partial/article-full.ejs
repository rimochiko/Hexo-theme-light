<%- partial('_partial/head') %>
	<div class="main">
		<div class="article-list">
			<div class="article">
				<div class="date">
					<p class="p_date"><%- item.date.format(theme.date_format) %></p>
				</div>
				<div class="title">
					<p class="p_title"><%- item.title || theme.default_post_title || "Untitled" %></p>
				</div>
				<div class="des">
					<%- item.content %>
				</div>
				<div class="tag">
					<ul>
						<li><span>关键词：</span><li>
						<% if (item.tags && item.tags.length){ %>
                            <%- partial('article-tag', {item: item}) %>
                        <% } %>					
					</ul>
				</div>	
			</div>

			  <% if (item.comments){ %>
			  <%- partial('git') %>
			  <% } else { %>
			    <div class="git"></div>
			  <% } %>
		</div>
	</div>
<%- partial('_partial/footer') %>